<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="au.com.thoughtpatterns.core.bo">

  <!-- Define a resultSet that application sqlMaps can refer to (by extension) -->
  <resultMap id="ManagedObjectResult" class="au.com.thoughtpatterns.core.bo.ManagedObject">
    <result property="id" column="id"/>
    <result property="activityId" column="audit_id"/>
  </resultMap>

  <!-- Define a resultSet and select that the IbatisBox will use to obtain sequence numbers -->
  
  <resultMap id="main_sequence_result" class="java.lang.Long">
    <result property="value" />
  </resultMap>
  
  <select id="main_sequence" parameterClass="java.lang.Long" resultMap="main_sequence_result">
    select next value for main_sequence from dual_main_sequence
  </select>
  
</sqlMap>